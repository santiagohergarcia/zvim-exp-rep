<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core">
    <TableSelectDialog busy="{detailView>/userOVS/busy}" noDataText="No results Found!" title="{detailView>/userOVS/ovsTitle}"
    search="handleReportForOVSSearch" confirm="handleReportForOVSClose" cancel="handleReportForOVSCancel" 
    items="{detailView>/userOVS/results}">
        <ColumnListItem>
            <cells>
                <Text text="{detailView>Xuser}" />
                <Text text="{detailView>Lname}, {detailView>Fname}" />
            </cells>
        </ColumnListItem>
        <columns>
            <Column>
                <header>
                    <Text text="{i18n>userId}" />
                </header>
            </Column>
            <Column >
                <header>
                    <Text text="{i18n>nam}" />
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>